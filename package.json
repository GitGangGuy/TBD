{
	"name": "tbd",
	"version": "1.0.0",
	"private": false,
	"description": "TBD makes trunk based development in your GitHub repositories easy",
	"license": "AGPL-3.0-only",
	"repository": "https://github.com/GitGangGuy/TBD.git",
	"author": "GitGangGuy",
	"main": "./lib/index.js",
	"engines": {
		"node": ">= 10.13.0"
	},
	"scripts": {
		"build": "run-p 'build:*'",
		"build:typescript": "run-s typescript:build",
		"commit": "run-s 'commit:*'",
		"commit:1": "run-p 'commit:1:*'",
		"commit:1:add": "run-s fix stage",
		"commit:2": "run-p 'commit:2:*'",
		"commit:2:commit": "run-s git:commit",
		"dev": "NODE_ENV=development run-s 'dev:*'",
		"dev:1": "run-p 'dev:1:*'",
		"dev:1:build": "run-s build",
		"dev:2": "run-p 'dev:2:*'",
		"dev:2:run": "run-s nodemon:run",
		"dev:2:watch": "run-p 'dev:2:watch:*'",
		"dev:2:watch:typescript": "run-s typescript:watch",
		"eslint:fix": "eslint --fix --cache --report-unused-disable-directives --ext .js,.ts .",
		"eslint:report": "eslint --report-unused-disable-directives --max-warnings 0 --ext .js,.ts .",
		"fetch": "run-s git:fetch",
		"fix": "run-s format lint",
		"format": "run-s 'format:*'",
		"format:prettier": "run-s prettier:fix",
		"git:commit": "cz",
		"git:fetch": "git fetch --prune --all",
		"git:merge:commit": "git commit -m 'fix: merge from trunk'",
		"git:merge:trunk": "git merge trunk --no-commit",
		"git:pull": "git pull --rebase",
		"git:push": "git push --prune",
		"git:stage": "git add -A",
		"git:submodule": "git submodule update --remote",
		"jest:run": "jest",
		"lint": "run-s 'lint:*'",
		"lint:eslint": "run-s eslint:fix",
		"nodemon:run": "nodemon --exec 'run-s' start",
		"postinstall": "husky install",
		"prettier:fix": "prettier --write --plugin-search-dir=. \"./**/*.{js,ts,md,json,yml}\"",
		"prettier:report": "prettier --check --plugin-search-dir=. \"./**/*.{js,ts,md,json,yml}\"",
		"probot:run": "probot run ./lib/index.js",
		"prod": "NODE_ENV=production run-s 'prod:*'",
		"prod:1": "run-p 'prod:1:*'",
		"prod:1:build": "run-s build",
		"prod:2": "run-p 'prod:2:*'",
		"prod:2:run": "run-s run",
		"pull": "run-s git:pull",
		"push": "run-s git:push",
		"run": "run-p 'run:*'",
		"run:probot": "run-s probot:run",
		"stage": "run-s git:stage",
		"start": "run-s run",
		"submodule": "run-s git:submodule",
		"sync": "run-s -c wiki submodule commit push fetch pull",
		"test": "run-s jest",
		"trunk": "run-s trunk:merge stage trunk:commit",
		"trunk:commit": "run-s git:merge:commit",
		"trunk:merge": "run-s git:merge:trunk",
		"typescript:build": "tsc --preserveWatchOutput",
		"typescript:watch": "tsc -w --preserveWatchOutput",
		"wiki": "cd wiki && run-s wiki"
	},
	"dependencies": {
		"probot": "^11.0.1",
		"tslib": "^2.1.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^12.0.1",
		"@commitlint/config-conventional": "^12.0.1",
		"@types/jest": "^26.0.19",
		"@types/lodash-es": "^4.17.4",
		"@types/nock": "^11.1.0",
		"@types/node": "^14.14.19",
		"@typescript-eslint/eslint-plugin": "^4.17.0",
		"@typescript-eslint/parser": "^4.16.1",
		"commitizen": "^4.2.3",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.21.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.9",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
		"eslint-plugin-sort-keys-fix": "^1.1.1",
		"husky": "^5.1.3",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.4",
		"lodash-es": "^4.17.21",
		"nock": "^13.0.11",
		"nodemon": "^2.0.7",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"prettier-plugin-package": "^1.3.0",
		"smee-client": "^1.2.2",
		"ts-jest": "^26.4.4",
		"typedoc": "^0.20.32",
		"typescript": "^4.2.3"
	},
	"types": "./lib/index.d.ts"
}
