{
	"name": "tbd",
	"version": "1.0.0",
	"private": false,
	"description": "TBD makes trunk based development in your GitHub repositories easy",
	"license": "AGPL-3.0-only",
	"repository": "https://github.com/GitGangGuy/TBD.git",
	"author": "GitGangGuy",
	"main": "",
	"engines": {
		"node": ">= 10.13.0"
	},
	"scripts": {
		"add": "git add -A",
		"build": "run-p build:*",
		"build:typescript": "tsc",
		"commit": "run-s commit:*",
		"commit:add": "run-s fix add",
		"commit:run": "cz",
		"dev": "run-s dev:*",
		"dev:build": "run-s build",
		"dev:watch": "run-p watch start",
		"eslint:fix": "eslint --fix --cache --report-unused-disable-directives --ext .js,.ts .",
		"eslint:report": "eslint --report-unused-disable-directives --max-warnings 0 --ext .js,.ts .",
		"fetch": "git fetch --prune --all",
		"fix": "run-s format lint",
		"format": "run-s format:*",
		"format:prettier": "run-s prettier:fix",
		"lint": "run-s lint:*",
		"lint:eslint": "run-s eslint:fix",
		"prettier:fix": "prettier --write --plugin-search-dir=. \"./**/*.{js,ts,md,json,yml}\"",
		"prettier:report": "prettier --check --plugin-search-dir=. \"./**/*.{js,ts,md,json,yml}\"",
		"prod": "run-s prod:*",
		"prod:build": "run-s build",
		"prod:start": "run-s start",
		"pull": "git pull --rebase",
		"push": "git push --prune",
		"start": "run-p start:*",
		"start:probot": "probot run ./lib/index.js",
		"submodules": "git submodule update --remote",
		"sync": "run-s git:fetch git:pull",
		"test": "jest",
		"trunk": "run-s trunk:merge add trunk:commit",
		"trunk:commit": "git commit -m 'fix: merge from master'",
		"trunk:merge": "git merge master --no-commit",
		"watch": "run-p watch:*",
		"watch:typescript": "tsc -w"
	},
	"dependencies": {
		"probot": "^11.0.1"
	},
	"devDependencies": {
		"@types/jest": "^26.0.19",
		"@types/nock": "^11.1.0",
		"@types/node": "^14.14.19",
		"@typescript-eslint/eslint-plugin": "^4.17.0",
		"@typescript-eslint/parser": "^4.16.1",
		"commitizen": "^4.2.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.21.0",
		"eslint-config-airbnb-typescript": "^12.3.1",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jest": "^24.1.9",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
		"eslint-plugin-sort-keys-fix": "^1.1.1",
		"husky": "^5.1.3",
		"jest": "^26.6.3",
		"lint-staged": "^10.5.4",
		"nock": "^13.0.11",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"prettier-plugin-package": "^1.3.0",
		"smee-client": "^1.2.2",
		"ts-jest": "^26.4.4",
		"typescript": "^4.2.3"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose"
		}
	},
	"jest": {
		"testEnvironment": "node"
	}
}
